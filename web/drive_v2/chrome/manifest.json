{
  "manifest_version": 2,
  "name": "Dart Google Drive Client Example",
  "version": "1",
  "icons": {"128": "dart_icon.png"},
  "app": {
    "background": {
      "scripts": ["background.dart.js"]
    }
  },
  "permissions": ["experimental","https://www.googleapis.com/*"],
  "oauth2": {
    "client_id": "YOUR CLIENT ID",
    "scopes": ["https://www.googleapis.com/auth/drive"]
  }
}
